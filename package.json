{
  "name": "wallet-ui",
  "version": "1.0.0",
  "description": "A React-based Wallet UI project with token contract integration",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "deploy": "npm run build",
    "generate-favicon": "node scripts/generate-favicon.js",
    "contract:deploy": "hardhat run scripts/deploy.js --network",
    "contract:verify": "hardhat verify --network"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "ethers": "^6.7.1",
    "web3": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "sharp": "^0.32.0",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "hardhat": "^2.19.0"
  },
  "keywords": ["react", "wallet", "ui", "blockchain", "token", "contract", "dApp"],
  "author": "Adnanmd76",
  "license": "MIT",
  "homepage": "https://github.com/Adnanmd76/wallet-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/Adnanmd76/wallet-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Adnanmd76/wallet-ui/issues"
  }
}